<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>WebJava</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
     <servlet-name>Index</servlet-name>
     <servlet-class>
     	defaut.Index
     </servlet-class>
  </servlet>
  <servlet-mapping>
     <servlet-name>Index</servlet-name>
     <url-pattern>/</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>Articles</servlet-name>
  	<servlet-class>
  		defaut.Articles
  	</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Articles</servlet-name>
  	<url-pattern>/Articles</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>CreerArticle</servlet-name>
  	<servlet-class>
  		defaut.CreerArticle
  	</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>CreerArticle</servlet-name>
  	<url-pattern>/CreerArticle</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>ListeAuteurs</servlet-name>
  	<servlet-class>
  		defaut.ListeAuteurs
  	</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ListeAuteurs</servlet-name>
  	<url-pattern>/ListeAuteurs</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>VueArticle</servlet-name>
  	<servlet-class>
  		defaut.VueArticle
  	</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>VueArticle</servlet-name>
  	<url-pattern>/VueArticle</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>ModifArticle</servlet-name>
  	<servlet-class>
  		defaut.ModifArticle
  	</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ModifArticle</servlet-name>
  	<url-pattern>/ModifArticle</url-pattern>
  </servlet-mapping>
  
</web-app>